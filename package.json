{
  "name": "employeeback",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "node ./build/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
    "build": "tsc",
    "sequelize": "./node_modules/.bin/sequelize $*",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:seed": "npm run sequelize db:seed:all",
    "db:reset": "npm run sequelize db:drop",
    "db:create": "npm run sequelize db:create",
    "db:migrate:seed": "npm run db:migrate && npm run db:seed",
    "db:reset:migrate:seed": "npm run db:reset  && npm run db:create && npm run db:migrate && npm run db:seed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker": "docker-compose up --force-recreate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "celebrate": "^12.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.2.1",
    "sequelize": "^5.21.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/hapi__joi": "^15.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^12.7.5",
    "@types/sequelize": "^4.28.3",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  }
}